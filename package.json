{
    "name": "webpack-intensive",
    "version": "0.0.0",
    "author": "Dima Vakatsiienko <dvakatsiienko@lectrum.io>",
    "private": true,
    "scripts": {
        "checkpoint": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/git/checkpoint.js\"",
        "sync": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/git/sync.js\"",
        "build": "cross-env-shell NODE_ENV=production node --require @babel/register \"./scripts/webpack/build.js\"",
        "start": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/webpack/start.js\"",
        "serve": "serve \"./build\" -s",
        "analyze": "webpack-bundle-analyzer \"./build/stats.json\""
    },
